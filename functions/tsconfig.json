{
  "compilerOptions": {
    "target": "es2017",

    "module": "commonjs",
    "typeRoots": ["./src/api/@types", "./node_modules/@types"],
    "resolveJsonModule": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "useUnknownInCatchVariables": false,
    "skipLibCheck": true
  }
}
